{
    "env": {
        "browser": true,
        "node": false
    },
    "globals": {
        "window": true,
        "angular": true,
        "CryptoJS": true,
        "describe": true,
        "beforeEach": true,
        "module": true,
        "inject": true,
        "afterEach": true,
        "it": true,
        "expect": true,
        "spyOn": true
    },
    "rules": {
        "no-redeclare": [2, {"builtinGlobals": true}],
        "no-undef": 2,
        "quotes": [2, "single"]
    }
}
